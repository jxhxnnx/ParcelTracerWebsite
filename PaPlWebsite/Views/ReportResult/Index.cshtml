@{ 
    var data = ViewBag.data;
}

<div class="card">
    <div class="card-header" style="background-color: #e8f1f8;">
        <h4>Parcel Reporting</h4>
    </div>
    <div class="mx-5 my-3 col-md-12 card-body">
        <p>Your parcel with the TrackingId <b>@data</b> has been successfully registered to the new location.</p>
    </div>
</div>
